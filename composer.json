{
	"name"        : "bleicker/typo3-fastroute-request-handler",
	"type"        : "library",
	"description" : "TYPO3 Request-Handler using fastroutes",
	"version"     : "1.0.0",
	"require"     : {
		"typo3/cms"    : ">=7.6,<8.0",
		"bleicker/fastroute-requesthandler": "~1.0"
	},
	"require-dev" : {
		"phpunit/phpunit": "~4.7"
	},
	"autoload"    : {
		"psr-4": {
			"Bleicker\\TYPO3\\FastRoute\\RequestHandler\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Bleicker\\TYPO3\\FastRoute\\RequestHandler\\Tests\\": "test"
		}
	},
	"extra"       : {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir"        : ".Build/Web",
			"Package"        : {
				"partOfMinimalUsableSystem": true
			}
		}
	}
}
